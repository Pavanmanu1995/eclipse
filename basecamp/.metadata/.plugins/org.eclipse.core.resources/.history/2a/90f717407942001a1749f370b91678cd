package basecamp1;
import java.util.Scanner;
public class demo {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the number");
		int num=sc.nextInt();
		System.out.println("the entered number is"+num);
		while(num!=0)
		{
		int n=num/10;
		System.out.println(n);
		}
		
		

	}

}
