package basecamp1;
import java.util.Scanner;
public class demo {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num=0;
		System.out.println("enter the number");
		num=sc.nextInt();
		System.out.println("the entered number is"+num);
		while(num>=10)
		{
		 num=num/10;
		System.out.println(num);
		}
		
		

	}

}
